{
  "expo": {
    "name": "T-Track",
    "slug": "tra-app-tracking",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "tra-app-tracking-scheme",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/transcoralv-splash-icon-dark.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/transcoralv-adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.transcoralv.tracking",
      "googleServicesFile": "./google-services.json",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyD39z0hIN3gG8W7HB1CnL0jsLuvZrhoJDA"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#FF231F7C",
          "sounds": [
            "./assets/notification.wav"
          ]
        }
      ],
      "expo-router",
      [
      "expo-splash-screen",
        {
          "backgroundColor": "#DEE6EA",
          "resizeMode": "contain",
          "image": "./assets/transcoralv-splash-icon-dark.png",
          "dark": {
            "image": "./assets/transcoralv-splash-icon-light.png",
            "backgroundColor": "#211915"
          },
          "imageWidth": 200
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "cb2f2148-228f-4ddf-804a-d20f140cae05"
      }
    },
    "owner": "devtranscoralv"
  }
}
